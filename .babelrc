{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["Android >= 4"]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-assign",
    "syntax-async-generators"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
